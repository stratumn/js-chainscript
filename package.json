{
  "name": "js-chainscript",
  "version": "1.0.0",
  "description": "Official JavaScript implementation of https://github.com/stratumn/chainscript.",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "clean": "rm -r ./lib",
    "prepare": "npm run build && cp -r ./src/proto ./lib",
    "build": "tsc",
    "lint": "tslint --project \"./tsconfig.json\"",
    "test": "jest",
    "protobuf": "protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --js_out=import_style=commonjs,binary:src --ts_out=src proto/chainscript.proto"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stratumn/js-chainscript.git"
  },
  "keywords": [
    "stratumn",
    "chainscript"
  ],
  "author": "Stratumn Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/stratumn/js-chainscript/issues"
  },
  "homepage": "https://github.com/stratumn/js-chainscript#readme",
  "dependencies": {
    "@types/jmespath": "^0.15.0",
    "canonicaljson": "^1.0.1",
    "fast-sha256": "^1.1.0",
    "jmespath": "^0.15.0"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.2.7",
    "@types/jest": "^23.3.1",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.3",
    "ts-protoc-gen": "^0.7.6",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testRegex": "/src/.*\\.spec\\.ts$"
  }
}
